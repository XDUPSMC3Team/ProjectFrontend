{
  "name": "dmall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "echo 'ERROR!' ",
    "clean": "rm -rf ./dist",
    "serve": "cd ./dist && http-server -p 8000",
    "refresh": "rm -rf node_modules && cnpm install",
    "theme:init": "./node_modules/.bin/et -i ./theme/element-variables.scss",
    "theme:build": "rm -rf ./theme/build && ./node_modules/.bin/et -c ./theme/element-variables.scss -o ./theme/build -m",
    "theme:clean": "rm -rf ./theme && mkdir ./theme",
    "theme:refresh": "npm run theme:clean && npm run theme:init"
  },
  "dependencies": {
    "element-ui": "^2.4.5",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-plugin-unit-mocha": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "chai": "^4.1.2",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.4.11",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
